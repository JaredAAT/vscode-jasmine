{
  "name": "JasmineSnippets",
  "description": "Code snippets for JavaScript Jasmine test framework in ES6 syntax",
  "version": "1.3.0",
  "displayName": "Jasmine code snippets",
  "publisher": "xabikos",
  "icon": "images/jasmine.png",
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/JaredAAT/vscode-jasmine"
  },
  "engines": {
    "vscode": "0.10.x"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Jasmine code snippets configuration",
      "properties": {
        "jasmineSnippets.quoteStyle": {
          "type": "string",
          "enum": [
            "'",
            "\"",
            "`"
          ],
          "default": "`",
          "description": "Code snippets quote style"
        }
      }
    },
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/snippets.json"
      }
    ]
  }
}
